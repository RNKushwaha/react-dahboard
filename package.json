{
  "name": "react-dashabord",
  "version": "0.1.0",
  "description": "React Dashboard",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "export": "react-snap",
    "build:serve": "yarn build && http-server --port 8080 dist -o --gzip",
    "build:profile": "yarn build --profile && http-server --port 8080 dist -o --gzip",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "reactSnap": {
    "source": "dist",
    "inlineCss": true,
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  },
  "keywords": [
    "React Dashboard",
    "React Admin Panel"
  ],
  "author": "RN Kushwaha <rn.kushwaha022@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 14.9.3"
  },
  "main": "index.js",
  "repository": "https://github.com/RNKushwaha/react-dashboard.git",
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "^5.0.0-alpha.102",
    "@mui/material": "^5.10.8",
    "@redux-devtools/extension": "^3.2.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "axios": "^1.1.3",
    "i18next": "^22.0.5",
    "i18next-browser-languagedetector": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.3",
    "react-router-sitemap": "^1.2.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^16.11.62",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "cra-bundle-analyzer": "^0.1.1",
    "react-scripts": "5.0.1",
    "react-snap": "^1.23.0",
    "typescript": "^4.8.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}